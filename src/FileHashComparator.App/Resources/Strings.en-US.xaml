    <sys:String x:Key="ButtonCancel">Cancel</sys:String>
    <sys:String x:Key="ButtonClear">Clear</sys:String>

    <sys:String x:Key="ColumnPath">Path</sys:String>
    <sys:String x:Key="ColumnExpectedHash">Expected Hash</sys:String>
    <sys:String x:Key="ColumnActualHash">Actual Hash</sys:String>
    <sys:String x:Key="ColumnStatus">Status</sys:String>

    <sys:String x:Key="StatusMatch">Match</sys:String>
    <sys:String x:Key="StatusMismatch">Mismatch</sys:String>
    <sys:String x:Key="StatusMissing">Missing</sys:String>
    <sys:String x:Key="StatusExtra">Extra</sys:String>
    <sys:String x:Key="StatusUnreadable">Unreadable</sys:String>
    <sys:String x:Key="StatusParseError">Parse Error</sys:String>
    <sys:String x:Key="StatusInvalidExpected">Invalid Expected</sys:String>
    <sys:String x:Key="StatusError">Error</sys:String>
    <sys:String x:Key="StatusComputed">Computed</sys:String>
    <sys:String x:Key="StatusSkipped">Skipped</sys:String>
